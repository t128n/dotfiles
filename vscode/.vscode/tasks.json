{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "install-recommended",
            "type": "shell",
            "command": "node .vscode/scripts/extensions.mjs install",
            "problemMatcher": []
        },
        {
            "label": "prune-unrecommended",
            "type": "shell",
            "command": "node .vscode/scripts/extensions.mjs prune",
            "problemMatcher": []
        },
        {
            "label": "sync-extensions",
            "dependsOrder": "sequence",
            "dependsOn": [
                "prune-unrecommended",
                "install-recommended"
            ],
            "problemMatcher": [],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "presentation": {
                "reveal": "always",
                "panel": "new",
                "clear": true
            }
        }
    ]
}
